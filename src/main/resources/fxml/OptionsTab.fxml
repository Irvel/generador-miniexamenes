<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>

<VBox fx:id="optionsTab"
      alignment="CENTER"
      prefHeight="111.0"
      spacing="10.0"
      VBox.vgrow="NEVER"
      xmlns="http://javafx.com/javafx/8.0.65"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="GeneradorMiniexamenes.controllers.OptionsController">
    <VBox.margin>
        <Insets top="10.0" />
    </VBox.margin>
    <HBox alignment="CENTER" spacing="20.0">
        <padding>
            <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
        </padding>
        <children>
            <VBox prefHeight="80.0" prefWidth="190.0" spacing="10.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
                <children>
                    <Label prefHeight="17.0" prefWidth="146.0" styleClass="inputLabel" text="Texto de Encabezado:" VBox.vgrow="ALWAYS" />
                    <JFXTextField fx:id="textFieldHeader" />
                </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="160.0" spacing="10.0" HBox.hgrow="ALWAYS">
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                </padding>
                <HBox.margin>
                    <Insets />
                </HBox.margin>
                <children>
                    <Label prefWidth="106.0" styleClass="inputLabel" text="Grupo Nuevo:" VBox.vgrow="ALWAYS" />
                    <JFXTextField fx:id="textFieldAddGroup" VBox.vgrow="ALWAYS" />
                    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                        <children>
                            <JFXButton fx:id="buttonAddNewGroup" disable="True" prefHeight="27.0" prefWidth="179.0" styleClass="inputBtn" text="Agregar Grupo Nuevo" />
                        </children>
                    </HBox>
                </children>
            </VBox>
        </children>
    </HBox>
</VBox>


