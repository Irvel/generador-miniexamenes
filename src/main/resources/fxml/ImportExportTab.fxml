<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="importExportTab"
      alignment="CENTER"
      prefHeight="111.0"
      spacing="10.0"
      HBox.hgrow="NEVER"
      xmlns="http://javafx.com/javafx/8.0.65"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="GeneradorMiniexamenes.controllers.ImportExportController">
    <VBox.margin>
        <Insets top="10.0" />
    </VBox.margin>
   <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
      <children>
           <JFXButton buttonType="RAISED" onAction="#importAction" prefHeight="27.0" prefWidth="180.0" styleClass="inputBtn" text="Importar" />
      </children>
   </VBox>
   <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" HBox.hgrow="ALWAYS">
      <children>
         <Label prefWidth="180.0" styleClass="inputLabel" text="Tema:">
            <VBox.margin>
               <Insets top="20.0" />
            </VBox.margin>
         </Label>
         <JFXComboBox fx:id="comboBoxSubject" prefHeight="27.0" prefWidth="180.0" />
           <JFXButton fx:id="buttonExport" buttonType="RAISED" onAction="#exportAction" prefHeight="27.0" prefWidth="180.0" styleClass="inputBtn" text="Exportar">
            <VBox.margin>
               <Insets bottom="20.0" />
            </VBox.margin>
         </JFXButton>
      </children>
   </VBox>
</HBox>
