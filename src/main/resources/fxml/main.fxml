<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox prefHeight="600.0" prefWidth="900.0" stylesheets="/css/main.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GeneradorMiniexamenes.controllers.MainController">
  <children>
      <TabPane prefHeight="200.0" prefWidth="200.0" side="LEFT" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab onSelectionChanged="#generateTabSelected" text="Generar Examenes">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" styleClass="H1" text="Generar Examenes" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="30.0">
                              </Label>
                        <VBox fx:id="mainGenContainer" alignment="CENTER" layoutX="218.0" layoutY="183.0" prefHeight="128.0" prefWidth="867.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="90.0">
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab onSelectionChanged="#gradeTabSelected" text="Calificar Examenes">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                         <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" styleClass="H1" text="Calificar Examenes" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="30.0">
                               </Label>
                        <VBox fx:id="mainGradeContainer" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="40.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="110.0">
                           <children>
                              <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="240.0" VBox.vgrow="ALWAYS">
                                 <children>
                                    <VBox prefHeight="80.0" prefWidth="160.0" spacing="10.0">
                                       <children>
                                          <Label prefWidth="106.0" styleClass="inputLabel" text="Tema:" />
                                          <JFXComboBox prefHeight="27.0" prefWidth="152.0" fx:id="cbTemaG" onAction="#selToGrade"/>
                                       </children>
                                       <padding>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </padding>
                                    </VBox>
                                    <VBox prefHeight="80.0" prefWidth="160.0" spacing="10.0">
                                       <children>
                                          <Label prefWidth="106.0" styleClass="inputLabel" text="Grupo:" />
                                          <JFXComboBox prefHeight="27.0" prefWidth="152.0" fx:id="cbGrupoG" onAction="#selGroup"/>
                                       </children>
                                       <padding>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </padding>
                                    </VBox>
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" prefHeight="245.0" prefWidth="865.0" spacing="120.0" VBox.vgrow="ALWAYS">
                                 <children>
                                    <JFXComboBox prefWidth="106.0" fx:id="cbExamenesG" onAction="#selExam" styleClass="inputLabel" VBox.vgrow="ALWAYS"/>
                                    <VBox alignment="CENTER" prefHeight="100.0" prefWidth="223.0">
                                       <children>
                                          <HBox alignment="TOP_CENTER" prefHeight="47.0" prefWidth="223.0" spacing="10.0" VBox.vgrow="ALWAYS">
                                             <children>
                                                <Label prefWidth="106.0" styleClass="inputLabel" text="Pregunta" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
                                                <Label prefWidth="106.0" styleClass="inputLabel" text="Ponderación" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
                                             </children>
                                          </HBox>
                                          <HBox alignment="TOP_CENTER" prefHeight="231.0" prefWidth="223.0" spacing="10.0">
                                             <children>
                                                <VBox prefHeight="80.0" prefWidth="160.0" spacing="10.0">
                                                   <children>
                                                      <JFXComboBox prefWidth="106.0" styleClass="inputLabel" fx:id="cbQuestionG" VBox.vgrow="ALWAYS" onAction="#selQuestion" />
                                                      <Label prefWidth="106.0" styleClass="inputLabel" text="Opción elegida:" />
                                                      <JFXComboBox fx:id="cbAnswerG" prefHeight="27.0" prefWidth="152.0" VBox.vgrow="ALWAYS" onAction="#selAnswer" />
                                                   </children>
                                                </VBox>
                                                <Label contentDisplay="CENTER" prefWidth="106.0" styleClass="inputLabel" text=".6" textAlignment="CENTER" HBox.hgrow="ALWAYS" />
                                             </children>
                                          </HBox>
                                       </children>
                                    </VBox>
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                                 <children>
                                    <Label text="Calificación:" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Editar Preguntas">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                         <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" styleClass="H1" text="Editar Bancos de Preguntas" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="30.0">
                               </Label>
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
          <Tab text="Importar / Exportar" onSelectionChanged="#importTabSelected">
              <content>
                  <AnchorPane
                          minHeight="0.0"
                          minWidth="0.0"
                          prefHeight="180.0"
                          prefWidth="200.0">
                      <children>
                          <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" styleClass="H1" text="Importar / Exportar Preguntas" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="30.0">
                          </Label>
                          <VBox alignment="CENTER"
                                AnchorPane.bottomAnchor="0.0"
                                AnchorPane.leftAnchor="0.0"
                                AnchorPane.rightAnchor="0.0"
                                AnchorPane.topAnchor="40.0"
                                fx:id="mainImExContainer">
                          </VBox>
                      </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
  </children>
</VBox>
